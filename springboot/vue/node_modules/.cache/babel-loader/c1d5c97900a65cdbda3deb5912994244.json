,YAAA,GAAA,CAAA,IAAA,KAAA,KAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,UAAA,MAAA,CAAA,IAAA;AACA,SAHA,MAGA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,MAAA;AACA;AACA,OAPA;AAQA,KA9EA;AA+EA,IAAA,qBA/EA,iCA+EA,GA/EA,EA+EA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,WAAA,iBAAA,GAAA,GAAA;AACA,KAlFA;AAmFA,IAAA,QAnFA,sBAmFA;AAAA;;AACA,UAAA,CAAA,KAAA,iBAAA,CAAA,MAAA,EAAA;AACA,aAAA,QAAA,CAAA,KAAA,CAAA,YAAA;AACA;AACA;;AACA,UAAA,GAAA,GAAA,KAAA,iBAAA,CAAA,GAAA,CAAA,UAAA,CAAA;AAAA,eAAA,CAAA,CAAA,EAAA;AAAA,OAAA,CAAA,CALA,CAKA;;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,qBAAA,EAAA,GAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,IAAA,KAAA,KAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,QAAA;;AACA,UAAA,MAAA,CAAA,IAAA;AACA,SAHA,MAGA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,QAAA;AACA;AACA,OAPA;AAQA,KAjGA;AAkGA,IAAA,KAlGA,mBAkGA;AACA,WAAA,IAAA,GAAA,EAAA;AACA,WAAA,IAAA;AACA,KArGA;AAsGA,IAAA,gBAtGA,4BAsGA,QAtGA,EAsGA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,QAAA;AACA,WAAA,QAAA,GAAA,QAAA;AACA,WAAA,IAAA;AACA,KA1GA;AA2GA,IAAA,mBA3GA,+BA2GA,OA3GA,EA2GA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,OAAA;AACA,WAAA,OAAA,GAAA,OAAA;AACA,WAAA,IAAA;AACA,KA/GA;AAgHA,IAAA,uBAhHA,mCAgHA,GAhHA,EAgHA;AACA,WAAA,IAAA,CAAA,IAAA,GAAA,GAAA;AACA,KAlHA;AAmHA,IAAA,sBAnHA,kCAmHA,GAnHA,EAmHA;AACA,WAAA,IAAA,CAAA,GAAA,GAAA,GAAA;AACA,KArHA;AAsHA,IAAA,QAtHA,oBAsHA,GAtHA,EAsHA;AACA,MAAA,MAAA,CAAA,IAAA,CAAA,GAAA;AACA,KAxHA;AAyHA,IAAA,GAzHA,iBAyHA;AACA,MAAA,MAAA,CAAA,IAAA,CAAA,uCAAA;AACA,KA3HA;AA4HA,IAAA,wBA5HA,sCA4HA;AACA,WAAA,QAAA,CAAA,OAAA,CAAA,MAAA;AACA,WAAA,IAAA;AACA;AA/HA;AAvBA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <div style=\"margin: 10px 0\">\r\n      <el-input style=\"width: 200px\" placeholder=\"请输入名称\" suffix-icon=\"el-icon-search\" v-model=\"name\"></el-input>\r\n      <!--      <el-input style=\"width: 200px\" placeholder=\"请输入\" suffix-icon=\"el-icon-message\" class=\"ml-5\" v-model=\"email\"></el-input>-->\r\n      <!--      <el-input style=\"width: 200px\" placeholder=\"请输入\" suffix-icon=\"el-icon-position\" class=\"ml-5\" v-model=\"address\"></el-input>-->\r\n      <el-button class=\"ml-5\" type=\"primary\" @click=\"load\">搜索</el-button>\r\n      <el-button type=\"warning\" @click=\"reset\">重置</el-button>\r\n    </div>\r\n\r\n    <div style=\"margin: 10px 0\">\r\n      <el-button type=\"primary\" @click=\"handleAdd\" v-if=\"user.role === 'ROLE_TEACHER'\">新增 <i\r\n          class=\"el-icon-circle-plus-outline\"></i></el-button>\r\n      <el-popconfirm\r\n          class=\"ml-5\"\r\n          confirm-button-text='确定'\r\n          cancel-button-text='我再想想'\r\n          icon=\"el-icon-info\"\r\n          icon-color=\"red\"\r\n          title=\"您确定批量删除这些数据吗？\"\r\n          @confirm=\"delBatch\"\r\n      >\r\n        <el-button type=\"danger\" slot=\"reference\" v-if=\"user.role != 'ROLE_USER'\">批量删除 <i\r\n            class=\"el-icon-remove-outline\"></i></el-button>\r\n      </el-popconfirm>\r\n      <!-- <el-upload action=\"http://localhost:9090/projects/import\" :show-file-list=\"false\" accept=\"xlsx\" :on-success=\"handleExcelImportSuccess\" style=\"display: inline-block\">\r\n        <el-button type=\"primary\" class=\"ml-5\">导入 <i class=\"el-icon-bottom\"></i></el-button>\r\n      </el-upload>\r\n      <el-button type=\"primary\" @click=\"exp\" class=\"ml-5\">导出 <i class=\"el-icon-top\"></i></el-button> -->\r\n    </div>\r\n\r\n    <el-table :data=\"tableData\" border stripe :header-cell-class-name=\"'headerBg'\"\r\n              @selection-change=\"handleSelectionChange\">\r\n      <el-table-column type=\"selection\" width=\"55\" v-if=\"user.role != 'ROLE_USER'\"></el-table-column>\r\n      <el-table-column prop=\"id\" label=\"ID\" width=\"80\" sortable></el-table-column>\r\n      <el-table-column prop=\"name\" label=\"毕设题目\"></el-table-column>\r\n      <el-table-column prop=\"content\" label=\"介绍\"></el-table-column>\r\n      <el-table-column label=\"文件\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button type=\"primary\" @click=\"download(scope.row.file)\">下载</el-button>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column prop=\"teacher\" label=\"老师\"></el-table-column>\r\n      <el-table-column prop=\"state\" label=\"审核状态\"></el-table-column>\r\n      <el-table-column prop=\"reason\" label=\"审核描述\"></el-table-column>\r\n\r\n      <el-table-column label=\"操作\" width=\"280\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button type=\"success\" @click=\"apply(scope.row)\"\r\n                     v-if=\"user.role === 'ROLE_USER'  && scope.row.state === '未选'\">申请\r\n          </el-button>\r\n          <el-button type=\"success\" @click=\"handleEdit(scope.row)\"\r\n                     v-if=\"user.role === 'ROLE_ADMIN'  && scope.row.state === '待审核'\">审核\r\n          </el-button>\r\n          <el-button type=\"success\" @click=\"handleEdit(scope.row)\" v-if=\"user.role != 'ROLE_USER'\">编辑 <i\r\n              class=\"el-icon-edit\"></i></el-button>\r\n          <el-popconfirm\r\n              class=\"ml-5\"\r\n              confirm-button-text='确定'\r\n              cancel-button-text='我再想想'\r\n              icon=\"el-icon-info\"\r\n              icon-color=\"red\"\r\n              title=\"您确定删除吗？\"\r\n              @confirm=\"del(scope.row.id)\"\r\n          >\r\n            <el-button type=\"danger\" slot=\"reference\" v-if=\"user.role != 'ROLE_USER'\">删除 <i\r\n                class=\"el-icon-remove-outline\"></i></el-button>\r\n          </el-popconfirm>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <div style=\"padding: 10px 0\">\r\n      <el-pagination\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"pageNum\"\r\n          :page-sizes=\"[2, 5, 10, 20]\"\r\n          :page-size=\"pageSize\"\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n\r\n    <el-dialog title=\"信息\" :visible.sync=\"dialogFormVisible\" width=\"40%\" :close-on-click-modal=\"false\">\r\n      <el-form label-width=\"140px\" size=\"small\" style=\"width: 85%;\" :model=\"form\" :rules=\"rules\" ref=\"ruleForm\">\r\n        <el-form-item prop=\"name\" label=\"毕设题目\" v-if=\"user.role === 'ROLE_TEACHER'\">\r\n          <el-input v-model=\"form.name\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item prop=\"content\" label=\"介绍\" v-if=\"user.role === 'ROLE_TEACHER'\">\r\n          <el-input type=\"textarea\" v-model=\"form.content\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item prop=\"file\" label=\"附件\" v-if=\"user.role === 'ROLE_TEACHER'\">\r\n          <el-upload action=\"http://localhost:9090/file/upload\" ref=\"file\" :on-success=\"handleFileUploadSuccess\">\r\n            <el-button size=\"small\" type=\"primary\">点击上传</el-button>\r\n          </el-upload>\r\n        </el-form-item>\r\n        <el-form-item prop=\"state\" label=\"审核状态\" v-if=\"user.role === 'ROLE_ADMIN'\">\r\n          <el-radio v-model=\"form.state\" label=\"未选\">审核通过</el-radio>\r\n          <el-radio v-model=\"form.state\" label=\"已选\">审核不通过</el-radio>\r\n        </el-form-item>\r\n        <el-form-item prop=\"reason\" label=\"审核描述\" v-if=\"user.role === 'ROLE_ADMIN'\">\r\n          <el-input type=\"textarea\" v-model=\"form.reason\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"save\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Projects\",\r\n  data() {\r\n    return {\r\n      tableData: [],\r\n      total: 0,\r\n      pageNum: 1,\r\n      pageSize: 10,\r\n      name: \"\",\r\n      form: {},\r\n      dialogFormVisible: false,\r\n      multipleSelection: [],\r\n      user: localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")) : {},\r\n      rules: {\r\n        name: [\r\n          {required: true, message: '请输入名称', trigger: 'blur'}\r\n        ],\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    this.load()\r\n  },\r\n  methods: {\r\n    apply(row) {\r\n      let data = {\r\n        name: row.name,\r\n        projectsId: row.id,\r\n        user: this.user.username,\r\n        reviewer: row.teacher,\r\n        state: '待审核'\r\n      }\r\n      this.request.post(\"/apply\", data).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"操作成功\")\r\n          this.load()\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n    load() {\r\n      this.request.get(\"/projects/page\", {\r\n        params: {\r\n          pageNum: this.pageNum,\r\n          pageSize: this.pageSize,\r\n          name: this.name,\r\n        }\r\n      }).then(res => {\r\n        this.tableData = res.data.records\r\n        this.total = res.data.total\r\n      })\r\n    },\r\n    save() {\r\n      this.$refs['ruleForm'].validate((valid) => {\r\n        if (valid) {\r\n          this.request.post(\"/projects\", this.form).then(res => {\r\n            if (res.code === '200') {\r\n              this.$message.success(\"保存成功\")\r\n              this.dialogFormVisible = false\r\n              this.load()\r\n            } else {\r\n              this.$message.error(res.msg)\r\n            }\r\n          })\r\n        }\r\n      })\r\n    },\r\n    handleAdd() {\r\n      this.dialogFormVisible = true\r\n      this.form = {}\r\n      this.$nextTick(() => {\r\n        if (this.$refs.img) {\r\n          this.$refs.img.clearFiles();\r\n        }\r\n        if (this.$refs.file) {\r\n          this.$refs.file.clearFiles();\r\n        }\r\n      })\r\n    },\r\n    handleEdit(row) {\r\n      this.form = JSON.parse(JSON.stringify(row))\r\n      this.dialogFormVisible = true\r\n      this.$nextTick(() => {\r\n        if (this.$refs.img) {\r\n          this.$refs.img.clearFiles();\r\n        }\r\n        if (this.$refs.file) {\r\n          this.$refs.file.clearFiles();\r\n        }\r\n      })\r\n    },\r\n    del(id) {\r\n      this.request.delete(\"/projects/\" + id).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"删除成功\")\r\n          this.load()\r\n        } else {\r\n          this.$message.error(\"删除失败\")\r\n        }\r\n      })\r\n    },\r\n    handleSelectionChange(val) {\r\n      console.log(val)\r\n      this.multipleSelection = val\r\n    },\r\n    delBatch() {\r\n      if (!this.multipleSelection.length) {\r\n        this.$message.error(\"请选择需要删除的数据\")\r\n        return\r\n      }\r\n      let ids = this.multipleSelection.map(v => v.id)  // [{}, {}, {}] => [1,2,3]\r\n      this.request.post(\"/projects/del/batch\", ids).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"批量删除成功\")\r\n          this.load()\r\n        } else {\r\n          this.$message.error(\"批量删除失败\")\r\n        }\r\n      })\r\n    },\r\n    reset() {\r\n      this.name = \"\"\r\n      this.load()\r\n    },\r\n    handleSizeChange(pageSize) {\r\n      console.log(pageSize)\r\n      this.pageSize = pageSize\r\n      this.load()\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      console.log(pageNum)\r\n      this.pageNum = pageNum\r\n      this.load()\r\n    },\r\n    handleFileUploadSuccess(res) {\r\n      this.form.file = res\r\n    },\r\n    handleImgUploadSuccess(res) {\r\n      this.form.img = res\r\n    },\r\n    download(url) {\r\n      window.open(url)\r\n    },\r\n    exp() {\r\n      window.open(\"http://localhost:9090/projects/export\")\r\n    },\r\n    handleExcelImportSuccess() {\r\n      this.$message.success(\"导入成功\")\r\n      this.load()\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n\r\n<style>\r\n.headerBg {\r\n  background: #eee !important;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views"}]}